
<div class="row clearfix">
    <div class="col-lg-3 col-md-6">
        <div class="card">
            <div class="body">
                <h3>@ViewBag.TotalVIPFarmer</h3>
                <p class="text-muted"> عدد المزارع VIP</p>
                <div class="progress">
                    <div class="progress-bar l-green" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="999999999"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-6">
        <div class="card">
            <div class="body">
                <h3>@ViewBag.TotalTrustFarmer</h3>
                <p class="text-muted"> عدد المزارع الموثوقه</p>
                <div class="progress">
                    <div class="progress-bar l-green" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="999999999"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-6">
        <div class="card">
            <div class="body">
                <h3>@ViewBag.TotalFarms</h3>
                <p class="text-muted"> عدد الكلي للمزارع</p>
                <div class="progress">
                    <div class="progress-bar l-green" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="999999999"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-6">
        <a asp-controller="Farmers" asp-action="ReservationFarmers" asp-route-id="0">
            <div class="card">
                <div class="body">
                    <h3>@ViewBag.TotalReservation</h3>
                    <p class="text-muted"> جميع الحجوزات</p>
                    <div class="progress">
                        <div class="progress-bar l-green" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="999999999"></div>
                    </div>
                </div>
            </div>
        </a>
    </div>

    <div class="col-lg-3 col-md-6">
         <a asp-controller="Farmers" asp-action="ReservationFarmers" asp-route-id="1">
            <div class="card">
                <div class="body">
                    <h3>@ViewBag.TotalReservationByCurrentMonth</h3>
                    <p class="text-muted"> عدد الحجوزات في الشهر الحالي</p>
                    <div class="progress">
                        <div class="progress-bar l-green" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="999999999"></div>
                    </div>
                </div>
            </div>
        </a>
    </div>

    <div class="col-lg-3 col-md-6">
       <a asp-controller="Farmers" asp-action="ReservationFarmers" asp-route-id="-1">
            <div class="card">
                <div class="body">
                    <h3>@ViewBag.TotalReservationByPreviousMonth</h3>
                    <p class="text-muted"> عدد الحجوزات في الشهر السابق</p>
                    <div class="progress">
                        <div class="progress-bar l-green" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="999999999"></div>
                    </div>
                </div>
            </div>
        </a>
    </div>

</div>


